#ifndef __KERNEL_VERSION_H_
#define __KERNEL_VERSION_H_

/* @templates@ values come from cmake/version.cmake */

#cmakedefine RTOCHIUS_VERSION_CODE @RTOCHIUS_VERSION_CODE@
#define RTOCHIUS_VERSION(a,b,c) (((a) << 16) + ((b) << 8) + (c))

#define RTOCHIUSVERSION          @RTOCHIUSVERSION@
#define RTOCHIUS_VERSION_NUMBER  @RTOCHIUS_VERSION_NUMBER@
#define RTOCHIUS_VERSION_MAJOR   @RTOCHIUS_VERSION_MAJOR@
#define RTOCHIUS_VERSION_MINOR   @RTOCHIUS_VERSION_MINOR@
#define RTOCHIUS_PATCHLEVEL      @RTOCHIUS_PATCHLEVEL@
#define RTOCHIUS_VERSION_STRING  @RTOCHIUS_VERSION_STRING@

#endif /* !__KERNEL_VERSION_H_ */
